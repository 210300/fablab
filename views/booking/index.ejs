    <script>
        function disableHoliday(date) {
    var string = $.datepicker.formatDate('yy-mm-dd', date);
          
    var filterDate = new Date(string);
    var day = filterDate.getDay();
    let x = [day != 0 && day !=1 && day !=2 && day !=3 && day !=5 && day !=6];
   
    return x;
    
 }
        
 $( "#datepicker" ).datepicker({

    beforeShowDay: disableHoliday,
    minDate:0
 });
 function disableHolidays(date) {
    var string = $.datepickers.formatDate('yy-mm-dd', date);
          
    var filterDate = new Date(string);
    var day = filterDate.getDay();
    let x = [day != 0 && day !=1 && day !=2 && day !=3 && day !=4 && day !=6];
   
    return x;
    
 }
        
 $( "#datepickers" ).datepickers({

    beforeShowDay: disableHolidays,minDate:0
    
 });
    </script>
